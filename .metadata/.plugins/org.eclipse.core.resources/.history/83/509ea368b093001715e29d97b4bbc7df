import java.net.MalformedURLException;
import java.net.URL;

public class MS06 {

	public static void main(String[] args) {
		
		// HTML Parsing API
		String url = "http://www.su.or.kr/03bible/daily/qtView.do?qtType=QT1";
		try {
			URL u = new URL(url);
		} catch (MalformedURLException e) {
			e.printStackTrace();
		}
		
		
	}

}
